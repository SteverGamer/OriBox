local function WDMJZ_fake_script() -- Fake Script: StarterGui.Client.OriBox.DevelopmentVersions.OriBoxBETA1.7.UISource.NumOfLines.LineIncreaser
    local script = Instance.new("LocalScript")
    script.Name = "LineIncreaser"
    script.Parent = Converted["_NumOfLines"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local function Newline()
		while task.wait() do
			local mesa = string.split(script.Parent.Parent.TextBox.Text, "\n")
			local numberoflines = #mesa
			local looptimes = 0
			local newsize = numberoflines * 39
			local text = ""
			script.Parent.Size = UDim2.new(0, 19, newsize, 0)
			while looptimes > numberoflines do
				local add = "[" .. tostring(looptimes) .. "]"
				text = text .. "\n" .. add
				looptimes = looptimes + 1
			end
			script.Parent.Text = text
		end
	end
	
	while task.wait() do
		Newline()
	end
end
