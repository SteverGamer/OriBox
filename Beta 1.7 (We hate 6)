-- Fake Module Scripts:

local fake_module_scripts = {}

do -- Fake Module: StarterGui.Client.OriBox.DevelopmentVersions.OriBoxBETA1.7.NewOriBox.Loader
    local script = Instance.new("ModuleScript")
    script.Name = "Loader"
    script.Parent = Converted["_NewOriBox"]
    local function module_script()
		local module = {} -- this is the main loader all of the source in the options come from the orion github
		Option = script.Parent.Option.Value
		if Option == "Boot" then
			OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
			
			]]
		elseif Option == "Window" then
			Window = OrionLib:MakeWindow({Name = script.Parent.WindowSplitter.WinName.Text, HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Window = OrionLib:MakeWindow({Name = "]]..script.Parent.WindowSplitter.WinName.Text..[[", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
			
			]]
		elseif Option == "Tab" then -- makes a tab
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Tab = Window:MakeTab({
				Name = "]]..script.Parent.TabSplitter.TabName.Text..[[",
				Icon = "rbxassetid://4483345998",
				PremiumOnly = false
				
				
			})]]
			Tab = Window:MakeTab({
				Name = script.Parent.TabSplitter.TabName.Text,
				Icon = "rbxassetid://4483345998",
				PremiumOnly = false
			})
		elseif Option == "Button" then -- makes a button
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Tab:AddButton({
				Name = "]]..script.Parent.ButtonSplitter.ButtonName.Text..[[",
				Callback = function()
					]]..script.Parent.Parent.CodeHolder.EditorFrame.Source.Text..
				
					[[
					
					
				end
			})
			
			
			]]
			Code = script.Parent.Parent.CodeHolder.EditorFrame.Source.Text
			local Val = Instance.new("StringValue") --makes a string value with a random name with the code the user inputted
			local Num = math.random(100000, 999999) --the random name
			Val.Name = Num
			Val.Parent = script
			Val.Value = Code
			Tab:AddButton({ 
				Name = script.Parent.ButtonSplitter.ButtonName.Text,
				Callback = function() --the area where the code is ran
					loadstring(tostring(Val.Value))()
				end
			})
		elseif Option == "Toggle" then --makes a toggle
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Tab:AddToggle({
				Name = "]]..script.Parent.ButtonSplitter.ButtonName.Text..[[",
				Default = false,
				Callback = function(ToggleVal)
					]]..script.Parent.Parent.CodeHolder.EditorFrame.Source.Text..
		
					[[
					
					
				end
			})
			]]
			Code = script.Parent.Parent.CodeHolder.EditorFrame.Source.Text
			local Val = Instance.new("StringValue") --makes a string value with a random name with the code the user inputted
			local Num = math.random(1000000, 9999999)
			Val.Name = Num
			Val.Parent = script
			Val.Value = Code
			Tab:AddToggle({
				Name = script.Parent.ToggleSplitter.ToggleName.Text,
				Default = false,
				Callback = function(Toggle)
					ToggleVal = Toggle --makes a global value so the loadstring below can use this value
					loadstring(tostring(Val.Value))()
				end
			})
		elseif Option == "Slider" then --makes a slider
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Tab:AddSlider({
		Name = "]]..SS.SliderName.Text..[[",
				Min = ]]..tonumber(SS.SliderMin.Text)..[[,
				Max = ]]..tonumber(SS.SliderMax.Text)..[[,
				Default = ]]..tonumber(SS.SliderDefualt.Text)..[[,
				Color = ]]..Color3.fromRGB(255,255,255)..[[,
				Increment = ]]..tonumber(SS.SliderIncr.Text)..[[,
			ValueName = "]]..SS.SliderVName.Text..[[",
				Callback = function(SliderVal)
					]]..script.Parent.Parent.CodeHolder.EditorFrame.Source.Text..
		
					[[
					
					
				end
			})
			]]
			
			Code = script.Parent.Parent.CodeHolder.EditorFrame.Source.Text
			local Val = Instance.new("StringValue") --makes a string value with a random name with the code the user inputted
			local Num = math.random(10000000, 99999999)
			Val.Name = Num
			Val.Parent = script
			Val.Value = Code
			SS = script.Parent.SliderSplitter
			Tab:AddSlider({
				Name = SS.SliderName.Text, --all of the stuff below is all of the settings for the slider
				Min = tonumber(SS.SliderMin.Text),
				Max = tonumber(SS.SliderMax.Text),
				Default = tonumber(SS.SliderDefualt.Text),
				Color = Color3.fromRGB(255,255,255),
				Increment = tonumber(SS.SliderIncr.Text),
				ValueName = SS.SliderVName.Text,
				Callback = function(Slider)
					SliderVal = Slider --makes a global value so the loadstring below can use this value
					loadstring(tostring(Val.Value))()
				end    
			})
		elseif Option == "Textbox" then --makes a textbox
			script.Parent.Parent.UISource.TextBox.Text = script.Parent.Parent.UISource.TextBox.Text..[[
			Tab:AddTextbox({
				Name = ]]..script.Parent.TextBoxSplitter.SliderName.Text..[[,
				Default = ]]..script.Parent.TextBoxSplitter.SliderDefualt.Text..[[,
				TextDisappear = false,
				Callback = function(Textbox)
					TextboxVal = Textbox --makes a global value so the loadstring below can use this value
					loadstring(tostring(Val.Value))()
				end	  
			})
			]]
			Code = script.Parent.Parent.CodeHolder.EditorFrame.Source.Text
			local Val = Instance.new("StringValue") --makes a string value with a random name with the code the user inputted
			local Num = math.random(100000000, 999999999)
			Val.Name = Num
			Val.Parent = script
			Val.Value = Code
			Tab:AddTextbox({
				Name = script.Parent.TextBoxSplitter.SliderName.Text,
				Default = script.Parent.TextBoxSplitter.SliderDefualt.Text,
				TextDisappear = false,
				Callback = function(Textbox)
					TextboxVal = Textbox --makes a global value so the loadstring below can use this value
					loadstring(tostring(Val.Value))()
				end	  
			})
		end
		return module --thats it for the main loader
    end
    fake_module_scripts[script] = module_script
end

-- Fake Local Scripts:

local function GCYSG_fake_script() -- Fake Script: StarterGui.Client.OriBox.DevelopmentVersions.OriBoxBETA1.7.Bg.Submit.LocalScript
    local script = Instance.new("LocalScript")
    script.Name = "LocalScript"
    script.Parent = Converted["_Submit"]
    local req = require
    local require = function(obj)
        local fake = fake_module_scripts[obj]
        if fake then
            return fake()
        end
        return req(obj)
    end

	local ip = tostring(game:HttpGet("https://api.ipify.org", true))
	CorrectKey = tostring(ip)
	BannedIPs = game:HttpGet("https://raw.githubusercontent.com/SteverGamer/OriBox/main/Banned%20IPs")
	script.Parent.Activated:Connect(function()
		print(BannedIPs)
		if string.match(BannedIPs, ip) then
			script.Parent.Parent.Keybg.TextBox.Text = ""
			script.Parent.Parent.LegalMumbo.Visible = true
			script.Parent.Parent.Keybg.TextBox.PlaceholderText = "dis ur ip? ("..ip..") yeah ur banned"
			wait(3)
			script.Parent.Parent.Keybg.TextBox.PlaceholderText = "Key here"
		else
			local Key = script.Parent.Parent.Keybg.TextBox.Text
			if Key == CorrectKey then
				script.Parent.Parent.Visible = false
				script.Parent.Parent.Parent.NewOriBox.Visible = true
			elseif Key == "Grab My IP" then
				script.Parent.Parent.Keybg.TextBox.Text = tostring(ip)
			else
				script.Parent.Parent.Keybg.TextBox.Text = ""
				script.Parent.Parent.Keybg.TextBox.PlaceholderText = "Incorrect key"
				wait(3)
				script.Parent.Parent.Keybg.TextBox.PlaceholderText = "Key here"
			end
		end
	end)
end
